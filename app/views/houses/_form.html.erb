<div class="container">
	<div class="row">
		<div class="col s12">
			<%= form_for @house, html: { multipart: true } do |f| %>
				<div class="title">
					<%= f.text_field :title, :placeholder => "Title" %>
				</div>

				<div class="content">
					<%= f.text_area :content, :placeholder => "Details" %>
				</div>
				 
				<%= f.file_field :image, placeholder: "Upload Photo"%>
				
				<input type="submit" placeholder="submit" class="btn-large"/>
			<% end %>
		</div>
	</div>
</div>