<h1>NEW FORM FOR HOUSE!</h1>

<div class="new-house">
	<%= form_for @house, :html => { :multipart => true } do |f| %>
		<div class="title">
			<%= f.text_field :title, :placeholder => "Title" %>
		</div>

		<div class="content">
			<%= f.text_field :content, :placeholder => "Details" %>
		</div>
		 
		<%= f.file_field :image, placeholder: "Upload Photo"%>
		<input type="submit" placeholder="submit"/>
	<% end %>
</div>