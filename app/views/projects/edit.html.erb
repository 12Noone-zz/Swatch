<h1>EDIT</h1>
<div class="new-post">

<%= form_for @project, :url => projects_path, :html => { :multipart => true } do |f| %>
	
	<div class="title">
		<%= f.text_field :title, :value => @project.title %>
	</div>

	<div class="content">
		<%= f.text_field :content, :placeholder => @project.content %>
	</div>
	<!-- <%= f.file_field :image, placeholder: "Upload Photo"%> -->
	</div>
		<input type="submit" placeholder="submit"/>
</div>
	<% end %>