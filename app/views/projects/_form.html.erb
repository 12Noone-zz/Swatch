<div class="container">
	<div class="row">
		<%= form_for [@house, @project], :html => { :multipart => true, :class => 'col s12'} do |f| %>
			<div class="title">
				<%= f.text_field :title, :placeholder => "Title" %>
			</div>

			<div class="content">
				<%= f.text_area :content, :placeholder => "Details" %>
			</div>
			 
			 <%= f.file_field :image1, placeholder: "Upload Photo"%>
			 <%= f.text_area :notes1, :placeholder => "More about this project!" %> <br>
			 <%= f.file_field :image2, placeholder: "Upload Photo"%>
			 <%= f.text_area :notes2, :placeholder => "More about this project!" %> <br>
			 <%= f.file_field :image3, placeholder: "Upload Photo"%>
			 <%= f.text_area :notes3, :placeholder => "More about this project!" %> <br>
			 <%= f.file_field :image4, placeholder: "Upload Photo"%>
			 <%= f.text_area :notes4, :placeholder => "More about this project!" %> <br>
			 <%= f.file_field :image5, placeholder: "Upload Photo"%>
			 <%= f.text_area :notes5, :placeholder => "More about this project!" %>
			<p>
	  			<%= f.label :tag_list %><br/>
	  			<%= f.text_field :tag_list %>
			</p>
			<input type="submit" placeholder="submit" class= "btn-large"/>
		<% end %>
	</div>
</div>
