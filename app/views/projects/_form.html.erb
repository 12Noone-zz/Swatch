<div class="new-post pure-form">
	<%= form_for @project, :html => { :multipart => true } do |f| %>
		<div class="title">
			<%= f.text_field :title, :placeholder => "Title" %>
		</div>

		<div class="content">
			<%= f.text_field :content, :placeholder => "Details" %>
		</div>
		 
		 <%= f.file_field :image1, placeholder: "Upload Photo"%> 
		 <%= f.file_field :image2, placeholder: "Upload Photo"%>
		 <%= f.file_field :image3, placeholder: "Upload Photo"%>
		 <%= f.file_field :image4, placeholder: "Upload Photo"%>
		 <%= f.file_field :image5, placeholder: "Upload Photo"%>
		<p>
  			<%= f.label :tag_list %><br/>
  			<%= f.text_field :tag_list %>
		</p>
		<input type="submit" placeholder="submit"/>
	<% end %>
</div>


